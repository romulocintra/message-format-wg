{
  "scenario": "Reserved statements",
  "description": "Tests for unsupported statements",
  "defaultTestProperties": {
    "locale": "en-US",
    "expErrors": [
      {
        "type": "unsupported-statement"
      }
    ]
  },
  "tests": [
    { "src" : ".i {1} {{}}" },
    { "src" : ".l $y = {|bar|} {{}}" },
    { "src" : ".l $x.y = {|bar|} {{}}" }
  ]
}

